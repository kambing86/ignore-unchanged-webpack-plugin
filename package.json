{
  "name": "ignore-unchanged-webpack-plugin",
  "version": "1.0.2",
  "description": "A Webpack plugin to not emitting unchanged files to the output path",
  "main": "dist/cjs.js",
  "scripts": {
    "start": "npm run build -- -w",
    "appveyor:test": "npm run test",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js'",
    "clean": "del-cli dist",
    "lint": "eslint --cache src test",
    "lint-staged": "lint-staged",
    "prebuild": "npm run clean",
    "prepublish": "npm run build",
    "release": "standard-version",
    "security": "nsp check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collectCoverageFrom='src/**/*.js' --coverage",
    "travis:lint": "npm run lint && npm run security",
    "travis:test": "npm run test -- --runInBand",
    "travis:coverage": "npm run test:coverage -- --runInBand",
    "webpack-defaults": "webpack-defaults",
    "defaults": "webpack-defaults"
  },
  "author": "Chua Kang Ming",
  "url": "https://github.com/kambing86/ignore-unchanged-webpack-plugin",
  "email": "kambing860210@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.11.0",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "nsp": "^3.2.1",
    "pre-commit": "^1.2.2",
    "standard-version": "^4.3.0",
    "webpack": "^4.6.0",
    "webpack-defaults": "^2.3.0"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 4.3 < 5.0.0 || >= 5.10"
  },
  "peerDependencies": {
    "webpack": "^2.0.0 || ^3.0.0 || ^4.0.0"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "async": "^2.6.0",
    "object-hash": "^1.3.0"
  }
}
